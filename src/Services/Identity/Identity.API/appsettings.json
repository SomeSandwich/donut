{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Spring": {
    "Application": {
      "Name": "identity-cluster"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://eureka:8761/eureka",
      "ShouldRegisterWithEureka": true
    },
    "Instance": {
      "InstanceId": "identity-api-destination",
      "HostName": "identity.api",
      "NonSecurePort": 8080,
      "StatusPageUrlPath": "/swagger/index.html"
      //"HealthCheckUrlPath": "/health","
    }
  }
}
