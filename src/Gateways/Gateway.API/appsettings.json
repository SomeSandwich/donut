{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        //"Microsoft": "Warning",
        //"Yarp": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://eureka:8761/eureka",
      "ShouldRegisterWithEureka": false
    }
  }
}
